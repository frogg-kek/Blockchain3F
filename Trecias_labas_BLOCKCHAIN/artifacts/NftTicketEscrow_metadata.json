		"devdoc": {
			"details": "Simple NFT-like ticketing and resale system with escrow-style transfers.      Educational example suitable for Remix and testnets (Sepolia, etc). Features:  - Event creation by organizers  - Primary ticket purchases  - Secondary resale marketplace with on-contract escrow handling  - Ticket validation (mark as used)  - Event cancellation and ticket refunds Notes:  - Tickets are simple structs (not an ERC-721 implementation) to keep code compact.  - All external calls that transfer ETH use the checks-effects-interactions pattern.",
			"kind": "dev",
			"methods": {
				"refundTicket(uint256)": {
					"details": "Refunds the original primary ticket price. If ticket was purchased via resale,      we still refund the original event ticket price for simplicity."
				},
				"validateTicket(uint256)": {
					"details": "For simplicity callable by anyone; in production restrict to gate staff or organizer."
				}
			},
			"title": "NftTicketEscrow",
			"version": 1
		},
		"userdoc": {
			"kind": "user",
			"methods": {
				"buyPrimaryTicket(uint256)": {
					"notice": "Buy a primary ticket directly from the organizer."
				},
				"buyResaleTicket(uint256)": {
					"notice": "Buy a ticket listed for resale. Contract handles escrow and platform fee."
				},
				"cancelEvent(uint256)": {
					"notice": "Cancel an event. Only organizer may call."
				},
				"createEvent(string,uint256,uint256,uint256)": {
					"notice": "Create an event. Organizer becomes msg.sender."
				},
				"eventExists(uint256)": {
					"notice": "Helper to check if an event exists"
				},
				"getTicket(uint256)": {
					"notice": "Get basic ticket info tuple"
				},
				"listTicketForResale(uint256,uint256)": {
					"notice": "List an owned ticket for resale at a chosen price."
				},
				"refundTicket(uint256)": {
					"notice": "Refund a ticket after its event has been cancelled."
				},
				"setPlatformFeeBps(uint256)": {
					"notice": "Set platform fee in basis points (10000 = 100%). Only admin."
				},
				"validateTicket(uint256)": {
					"notice": "Validate (use) a ticket. This marks it as Used."
				}
			},
			"version": 1
		}
